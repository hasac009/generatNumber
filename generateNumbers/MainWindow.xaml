<Window x:Class="generateNumbers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:generateNumbers"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <Grid >
        <Button x:Name="ButtonStart" Content="Start" HorizontalAlignment="Center" VerticalAlignment="Top" Height="38" Width="152" Margin="0,164,0,0" Click="Button_Click"/>
        <Button x:Name="ButtonStop" Content="Stop" HorizontalAlignment="Center" VerticalAlignment="Top" Height="38" Width="152" Margin="0,217,0,0" Click="ButtonStop_Click"/>
        <TextBox x:Name="Tbox1" HorizontalAlignment="Left" Margin="149,65,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Tbox2" HorizontalAlignment="Left" Margin="149,109,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ListBox x:Name="ListNumbers" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,281,0,10"/>

    </Grid>
</Window>
